{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn server:dev\" \"yarn client:dev\" -n server,client",
    "client:dev": "vite --host 0.0.0.0",
    "server:dev": "ts-node-dev --project tsconfig.server.json src/server",
    "server:build": "tsc --project tsconfig.server.json",
    "client:build": "vite build",
    "build": "yarn server:build && yarn client:build",
    "serve": "vite preview",
    "start": "NODE_ENV=production node dist/server.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint ."
  },
  "dependencies": {
    "@mantine/core": "3.6.7",
    "@mantine/dates": "3.6.7",
    "@mantine/hooks": "3.6.7",
    "@mantine/next": "3.6.7",
    "@mantine/notifications": "3.6.7",
    "@mantine/prism": "3.6.7",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "ping": "^0.4.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.4.0",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "sweetalert2": "^11.4.4",
    "ui": "*",
    "uuid": "^8.3.2",
    "wake_on_lan": "^1.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/ping": "^0.4.1",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "@types/uuid": "^8.3.4",
    "@types/wake_on_lan": "^0.0.30",
    "@vitejs/plugin-react": "^1.3.0",
    "concurrently": "^7.2.2",
    "config": "*",
    "eslint": "^8.18.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig": "*",
    "types": "*",
    "typescript": "^4.7.4",
    "vite": "^2.9.9"
  }
}
